<%- include('partials/header',{title:'Admin Cars Page'}) %>

    <body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
            <a class="navbar-brand" href="/">Home</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Admin Option Choice
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown" aria-placeholder="woy">
                            <a class="dropdown-item" href="/admin/userList">See
                                Users List</a>
                            <a class="dropdown-item" href="/admin/cars/add">Add
                                New Cars</a>
                            <a class="dropdown-item" href="/admin/logout">Logout</a>
                        </div>
                    </li>
                </ul>
            </div>
        </nav>
        <div class="shadow p-3 mb-5 bg-white rounded">
            <div class="mx-auto" style="width: 200px;">
                <h5><%=cars.fullData%></h5>
            </div>
        </div>

        <br><br><br>
        <div class="mx-auto" style="width: 200px;">
            <h1 class="display-4">user List</h1>
        </div>
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col">Age</th>
                    <th scope="col">Gender</th>
                </tr>
            </thead>
            <tbody>
                <% invoices.forEach(item =>{ %> 
                    <tr>
                        <td><%=item.User.first_name%></td>
                        <td><%=item.User.last_name%></td>
                        <td><%=item.User.age%></td>
                        <td><%=item.User.gender%></td>
                    </tr>
                <% }) %> 
            </tbody>
        </table>

        <%- include('partials/footer') %>